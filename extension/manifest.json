{
  "name": "AI Meeting Assistant",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
   "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions":  ["<all_urls>"],
  
  
  "background": {
    "service_worker": "background.js"
  },
   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
